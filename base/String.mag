// Comparisons.
extend class String
    ==(other -> Bool) compareTo(other string) == 0
    !=(other -> Bool) compareTo(other string) != 0
    <(other -> Bool) compareTo(other string) < 0
    >(other -> Bool) compareTo(other string) > 0
    <=(other -> Bool) compareTo(other string) <= 0
    >=(other -> Bool) compareTo(other string) >= 0

    +(other -> String) concatenate(other string)
end