// Comparisons.
def String ==(other Object -> Bool) this compareTo(other toString) == 0
def String !=(other Object -> Bool) this compareTo(other toString) != 0
def String <(other Object -> Bool) this compareTo(other toString) < 0
def String >(other Object -> Bool) this compareTo(other toString) > 0
def String <=(other Object -> Bool) this compareTo(other toString) <= 0
def String >=(other Object -> Bool) this compareTo(other toString) >= 0

def String +(other Object -> String) this concatenate(other toString)