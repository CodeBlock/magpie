// Comparisons.
extend class String
    def ==(other -> Bool) compareTo(other string) == 0
    def !=(other -> Bool) compareTo(other string) != 0
    def <(other  -> Bool) compareTo(other string) < 0
    def >(other  -> Bool) compareTo(other string) > 0
    def <=(other -> Bool) compareTo(other string) <= 0
    def >=(other -> Bool) compareTo(other string) >= 0

    def +(other  -> String) concatenate(other string)
end