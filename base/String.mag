// Comparisons.
def String ==(other -> Bool) this compareTo(other toString) == 0
def String !=(other -> Bool) this compareTo(other toString) != 0
def String <(other -> Bool) this compareTo(other toString) < 0
def String >(other -> Bool) this compareTo(other toString) > 0
def String <=(other -> Bool) this compareTo(other toString) <= 0
def String >=(other -> Bool) this compareTo(other toString) >= 0

def String +(other -> String) this concatenate(other toString)