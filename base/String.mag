// Comparisons.
def String ==(other -> Bool) this compareTo(other string) == 0
def String !=(other -> Bool) this compareTo(other string) != 0
def String <(other -> Bool) this compareTo(other string) < 0
def String >(other -> Bool) this compareTo(other string) > 0
def String <=(other -> Bool) this compareTo(other string) <= 0
def String >=(other -> Bool) this compareTo(other string) >= 0

def String +(other -> String) this concatenate(other string)