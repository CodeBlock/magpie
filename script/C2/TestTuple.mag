using Core

TestTuple (->)
    Print "TestTuple"
    
    AssertEqual (1, (1, 2).First, "first of pair is one")
    AssertEqual (2, (1, 2).Second, "second of pair is two")
    
    AssertEqual (1, (1, 2, 3).First, "first of triplet is one")
    AssertEqual (2, (1, 2, 3).Second, "second of triplet is two")
    AssertEqual (3, (1, 2, 3).Third, "third of triplet is three")
end
