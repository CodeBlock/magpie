// expected: 0
// expected: 1
// expected: 2
// expected: 3
// expected: 4
// expected: 5

Main (->)
    // single line
    var a <- 0
    
    while a < 5 do a <- + 1
    
    Print a.String

    // multi-line body
    a <- 0
    
    while a < 5 do
        Print a.String
        a <- + 1
    end

    Print a.String
end
