def main ()
    zero ()                 //: zero local
    one "one"               //: one
                            //: one local
    two ("two a", "two b")  //: two a
                            //: two b
                            //: two local
end

def zero ()
    def local = "zero local"
    print local
end
    
def one (a String)
    def local = "one local"
    print a
    print local
end

def two (a String, b String)
    def local = "two local"
    print a
    print b
    print local
end
