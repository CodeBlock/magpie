Main (->)
    Zero
    One "one"
    Two ("two a", "two b")
end

Zero (->)
    def local <- "local"
    Print local
end
    
One (a String ->)
    def local <- "local"
    Print a
    Print local
end

Two (a String, b String ->)
    def local <- "local"
    Print a
    Print b
    Print local
end

