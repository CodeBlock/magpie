// expected: zero local
// expected: one
// expected: one local
// expected: two a
// expected: two b
// expected: two local

Main (->)
    Zero
    One "one"
    Two ("two a", "two b")
end

Zero (->)
    def local <- "zero local"
    Print local
end
    
One (a String ->)
    def local <- "one local"
    Print a
    Print local
end

Two (a String, b String ->)
    def local <- "two local"
    Print a
    Print b
    Print local
end
