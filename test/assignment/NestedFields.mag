class Bar
    this()
        bang = "b"
    end
    
    bang String
end

class Foo
    this()
        bar = Bar new()
    end
    
    bar Bar
end

def Foo change()
    this bar bang = "d"
end

var foo = Foo new()
print(foo bar bang)    //: b

foo bar bang = "c"
print(foo bar bang)    //: c

foo change()
print(foo bar bang)    //: d
