class Foo
    this()
        bar = "a"
        bang = "b"
    end
    
    bar String
    bang String
end
    
def Foo change()
    bar = "c"
    this bang = "d"
end

var foo = Foo new()
foo change()

print(foo bar)         //: c
print(foo bang)        //: d

foo bar = "e"
print(foo bar)         //: e
