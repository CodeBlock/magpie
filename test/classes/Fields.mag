class Foo
    def bar = "a"
    def bang = "b"
    
    def printFields ()
        print bar
        print this.bang
    end
    
    def change ()
        bar = "c"
        this.bang = "d"
    end
end

def foo = Foo.new
print foo.bar         //: a
print foo.bang        //: b

foo.printFields       //: a
                      //: b
foo.change

foo.printFields       //: c
                      //: d

print foo.bar         //: c
print foo.bang        //: d
