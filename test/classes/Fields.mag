class Foo
    this()
        bar = "a"
        bang = "b"
    end
    bar String
    bang String
end

def Foo printFields()
    print(bar)
    print(this bang)
end

var foo = Foo new()
print(foo bar)        //: a
print(foo bang)       //: b

foo printFields()     //: a
                      //: b
