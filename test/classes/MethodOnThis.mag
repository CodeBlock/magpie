class Foo
    noArgs() print("noArgs")
    oneArg(arg String) print("oneArg " + arg)
    
    someMethod()
        noArgs              //: noArgs
        oneArg("foo")       //: oneArg foo
        this noArgs             //: noArgs
        this oneArg("foo")      //: oneArg foo
    end
end

var foo = Foo new
foo someMethod
