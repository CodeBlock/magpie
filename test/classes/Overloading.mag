class Foo
    byNumArgs () print "zero"
    byNumArgs (a Int) print "one"
    byNumArgs (a Int, b Int) print "two"

    shared byNumArgs () print "shared zero"
    shared byNumArgs (a Int) print "shared one"
    shared byNumArgs (a Int, b Int) print "shared two"
end

var foo = Foo.new
foo.byNumArgs         //: zero
foo.byNumArgs 1       //: one
foo.byNumArgs (1, 2)  //: two

Foo.byNumArgs         //: shared zero
Foo.byNumArgs 1       //: shared one
Foo.byNumArgs (1, 2)  //: shared two
