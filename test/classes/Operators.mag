def Int ^ (exponent Int -> Int)
    var i       = 0
    var result  = 1

    while i < exponent do
        result = result * this
        i = i + 1
    end

    result
end

var a = 3 ^ 0
print(a) //: 1

var b = 2 ^ 1
print(b) //: 2

var c = 3 ^ 3
print(c) //: 27
    
var d = 2 ^ 2 ^ 3
print(d) //: 64

var e = 2 ^ (2 ^ 3)
print(e) //: 256
