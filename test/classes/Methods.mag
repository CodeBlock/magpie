class Foo
    def noArgs () print "noArgs"
    def oneArg (arg String) print ("oneArg " + arg)
    
    def twoArgs (arg1 String, arg2 String)
        print (arg1 + arg2)
    end
end

var foo = Foo.new
foo.noArgs              //: noArgs
foo.oneArg "one"        //: oneArg one
foo.twoArgs ("a", "b")  //: ab

// can also call no-argument methods in prefix position
noArgs foo              //: noArgs
