class Foo
end

def Foo noArgs() print("noArgs")
def Foo oneArg(arg String) print("oneArg " + arg)

def Foo twoArgs(arg1 String, arg2 String)
    print(arg1 + arg2)
end

var foo = Foo new()
foo noArgs()              //: noArgs
foo oneArg("one")       //: oneArg one
foo twoArgs("a", "b")   //: ab
