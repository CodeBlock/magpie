// The primitive types aren't compatible with each other.
print(Bool canAssignFrom(Bool))          //: true
print(Bool canAssignFrom(Int))           //: false
print(Bool canAssignFrom(Nothing))       //: false
print(Bool canAssignFrom(String))        //: false

print(Int canAssignFrom(Bool))           //: false
print(Int canAssignFrom(Int))            //: true
print(Int canAssignFrom(Nothing))        //: false
print(Int canAssignFrom(String))         //: false

print(Nothing canAssignFrom(Bool))       //: false
print(Nothing canAssignFrom(Int))        //: false
print(Nothing canAssignFrom(Nothing))    //: true
print(Nothing canAssignFrom(String))     //: false

print(String canAssignFrom(Bool))        //: false
print(String canAssignFrom(Int))         //: false
print(String canAssignFrom(Nothing))     //: false
print(String canAssignFrom(String))      //: true

// Everything is an object. Nothing isn't.
print(Object canAssignFrom(Bool))        //: true
print(Object canAssignFrom(Int))         //: true
print(Object canAssignFrom(Nothing))     //: false
print(Object canAssignFrom(Object))      //: true
print(Object canAssignFrom(String))      //: true
