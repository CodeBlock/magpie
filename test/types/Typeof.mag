// Literals.
print(typeof nothing) //: Nothing
print(typeof true) //: Bool
print(typeof 123) //: Int
print(typeof "i") //: String

// Arrays.
print(typeof [])            //: Object[]
print(typeof [1, 2, 3])     //: Int[]
print(typeof [true, false]) //: Bool[]
print(typeof [1, true])     //: Object[]

// Assignment.
print(typeof a = 3)       //: Int
print(typeof foo = "bar") //: String

// Blocks.
print(typeof
    123
end)        //: Int

print(typeof
    123
    true
end)        //: Bool


print(typeof
    123
    true
    "hey"
end)        //: String

// Loops never return anything.
print(typeof while true do 4) //: Nothing

// Variable definitions.
print(typeof var a = 234) //: Int
print(typeof var a = true) //: Bool

// Return expressions never actually evaluate to a result since they
// immediately unwind the stack.
print(typeof return 123) //: Never

// Conjunctions.
print(typeof true or false)  //: Bool
print(typeof true and false) //: Bool
print(typeof 1 or false)     //: Bool | Int
print(typeof 1 and false)    //: Bool | Int
print(typeof 1 or "two")     //: Int | String
print(typeof 1 and "two")    //: Int | String

// Conditionals.
print(typeof if true then 1)          //: Int | Nothing
print(typeof if true then 1 else 2)   //: Int
print(typeof if true then 1 else "2") //: Int | String

// Tuples.
print(typeof 1, true, "three")        //: (Int, Bool, String)
