print(1.1 == 1.2) // expect: false
print(1.1 == 1.1) // expect: true
print(1.2 == 1.1) // expect: false

print(1.1 != 1.2) // expect: true
print(1.1 != 1.1) // expect: false
print(1.2 != 1.1) // expect: true

// Main comparison operators.
print(1.1 <=> 1.2) // expect: -1
print(1.1 <=> 1.1) // expect: 0
print(1.2 <=> 1.1) // expect: 1

print(1.1 <=> 1.2) // expect: -1
print(1.1 <=> 1.1) // expect: 0
print(1.2 <=> 1.1) // expect: 1

// Derived comparison operators.
print(1.1 < 1.2) // expect: true
print(1.1 < 1.1) // expect: false
print(1.2 < 1.1) // expect: false

print(1.1 > 1.2) // expect: false
print(1.1 > 1.1) // expect: false
print(1.2 > 1.1) // expect: true

print(1.1 <= 1.2) // expect: true
print(1.1 <= 1.1) // expect: true
print(1.2 <= 1.1) // expect: false

print(1.1 >= 1.2) // expect: false
print(1.1 >= 1.1) // expect: true
print(1.2 >= 1.1) // expect: true
