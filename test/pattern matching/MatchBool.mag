// expected: true
// expected: false
// expected: any

Main (->)
    def a <- true
    match a
        case true  then Print "true"
        case false then Print "false"
    end
    
    match false
        case true  then Print "true"
        case false then Print "false"
    end
    
    match false
        case true then Print "true"
        case _    then Print "any"
    end
end
