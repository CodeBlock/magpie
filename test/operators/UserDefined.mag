Main (->)
    def a <- 3 ^ 0
    Print a.String
    
    def b <- 2 ^ 1
    Print b.String
    
    def c <- 3 ^ 3
    Print c.String
        
    def d <- 2 ^ 2 ^ 3
    Print d.String

    def e <- 2 ^ (2 ^ 3)
    Print e.String
end

^ (base Int, exponent Int -> Int)
    mutable i       <- 0
    mutable result  <- 1
    
    while i < exponent do
        result <- * base
        i <- + 1
    end
    
    result
end
