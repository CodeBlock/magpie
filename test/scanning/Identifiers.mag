Main (->)
    // valid prefix identifiers
    _
    Foo
    foo!
    Foo_!&|-+=<>?*/~@#$%^`
    _Foo
    Foo123

    // valid operators
    1 ! 1
    1 !&|-+=<>?*/~@#$%^` 1
    1 --------- 1
    1 -Foo 1
    1 *123 1
end

_                       (->) Print "0"
Foo                     (->) Print "1"
foo!                    (->) Print "2"
Foo_!&|-+=<>?*/~@#$%^`  (->) Print "3"
_Foo                    (->) Print "4"
Foo123                  (->) Print "5"

// valid operators
!                       (a Int, b Int ->) Print "6"
!&|-+=<>?*/~@#$%^`      (a Int, b Int ->) Print "7"
---------               (a Int, b Int ->) Print "8"
-Foo                    (a Int, b Int ->) Print "9"
*123                    (a Int, b Int ->) Print "10"