// expected: 0 10
// expected: 1 11
// expected: 2 12
// expected: 1 3
// expected: 2 4
// expected: 3 5
// expected: 10 2 1 30
// expected: 11 3 2 31

Main (->)
    var a <- 0
    var b <- 10
    
    // multiple whiles
    while a < 3
    while b < 14 do
        Print (a.String + " " + b.String)
        a <- + 1
        b <- + 1
    end
    
    // multiple fors
    for c <- 1 -to- 5
    for d <- 3 -to- 6 do Print (c.String + " " + d.String)
    
    a <- 10
    b <- 2
    
    // mixed
    while a < 30
    for c <- 1 -to- 5
    while b < 4
    for d <- 30 -to- 50 do
        Print (a.String + " " + b.String + " " + c.String + " " + d.String)
        a <- + 1
        b <- + 1
    end
end
