// one line
for i = 1 to(3) do print(i)
//: 1
//: 2
//: 3

// block body
for i = 1 until(4) do
    print(i)
end
//: 1
//: 2
//: 3

// parallel iterators
for i = 10 to(60) for j = 5 to(12)
for k = 3 times() do
    print(i)
    print(j)
    print(k)
end
//: 10
//: 5
//: 0
//: 11
//: 6
//: 1
//: 12
//: 7
//: 2
