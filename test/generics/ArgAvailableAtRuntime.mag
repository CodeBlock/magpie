var foo[A]()
    print(A)
end

var bar[A]()
    var nested[B]()
        print(A string + " " + B)
    end
    nested[Int]()
end

// TODO(bob): Having to explicitly do () after [] is lame.
var main(->)
    foo[Int]()   //: Int
    foo[Bool]()  //: Bool
    foo[123]()   //: 123
    
    bar[Int]()  //: Int Int
    bar[Bool]() //: Bool Int
end
