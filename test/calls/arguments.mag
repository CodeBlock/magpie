// Method with no parameter on either side.
def foo()
    print("no params")
end

// Method with right parameter.
def foo(n)
    print("right")
    print(n)
end

// Method with left parameter.
def (n) foo
    print("left")
    print(n)
end

// Call with no argument.
foo() // expect: no params

// Call with explicit nothing argument.
foo(nothing) // expect: right
             // expect: nothing

// Call with right argument.
foo("arg") // expect: right
           // expect: arg

// Call with left argument.
"arg" foo // expect: left
          // expect: arg

// Record patterns.
def foo(a, b)
    print("2-uple")
end

def foo(a, b, c)
    print("3-uple")
end

def foo(x: x, y: y)
    print("x y")
end

foo(1, 2)       // expect: 2-uple
foo(1, 2, 3)    // expect: 3-uple
foo(x: 1, y: 2) // expect: x y
