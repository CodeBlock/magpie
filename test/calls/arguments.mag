// Method with no parameter on either side.
def foo()
    print("no params")
end

// Method with right parameter.
def foo(n)
    print("right")
    print(n)
end

// Method with left parameter.
def (n) foo
    print("left")
    print(n)
end

// Call with no argument.
foo() // expect: no params

// Call with explicit nothing argument.
foo(nothing) // expect: right
             // expect: nothing

// Call with right argument.
foo("arg") // expect: right
           // expect: arg

// Call with left argument.
"arg" foo // expect: left
          // expect: arg
