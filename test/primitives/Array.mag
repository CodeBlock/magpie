// Constructor.
var a = Array of(1, true, "three")
print(a count) //: 3

// Indexing.
print(a(0))    //: 1
print(a(1))    //: true
print(a(2))    //: three

// Index from end.
print(a(-3))    //: 1
print(a(-2))    //: true
print(a(-1))    //: three

// Modify.
a add("fore")
print(a count) //: 4
print(a(3))    //: fore

a insert(2, "mid")
print(a count) //: 5
print(a(2))    //: mid
print(a(3))    //: three

a removeAt(2)
print(a count) //: 4
print(a(2))    //: three

a(1) = "set"
print(a(1))    //: set
a(-2) = "neg"
print(a(-2))   //: neg

a clear()
print(a count) //: 0
