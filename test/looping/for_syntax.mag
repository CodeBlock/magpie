// TODO(bob):
// - Destructuring pattern.

// Block body.
do
    for i in 1..3 do
        print(i)
    end
    // expect: 1
    // expect: 2
    // expect: 3
end

// Block for iterator.
do
    for i in
        1..3
    do
        print(i)
    end
    // expect: 1
    // expect: 2
    // expect: 3
end

// Single-expression body.
do
    for i in 1..3 do print(i)
    // expect: 1
    // expect: 2
    // expect: 3
end
