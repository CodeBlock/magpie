specify("Array contains?()") with
    it should("return true if the array contains the item") with
        var a = Array of("a", "b", "c")
        a contains?("a") shouldEqual(true)
        a contains?("b") shouldEqual(true)
        a contains?("c") shouldEqual(true)
        a contains?("d") shouldEqual(false)
    end

    it should("return false if the array is empty") with
        var a = Array of()
        a contains?("a") shouldEqual(false)
    end
end
